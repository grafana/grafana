  "paths": {
    "/v1/namespace/{namespace}/batchcheck": {
      "post": {
        "summary": "Batch check access for subject on multiple resources",
        "description": "This API checks if a subject is allowed to perform actions on multiple resources in a single request. Maximum 50 checks per request.",
        "operationId": "BatchCheck",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1BatchCheckResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "namespace",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "subject": {
                  "type": "string"
                },
                "checks": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/v1BatchCheckItem"
                  }
                },
                "options": {
                  "$ref": "#/definitions/v1BatchCheckOptions"
                }
              },
              "description": "BatchCheckRequest contains multiple checks to be performed at once.\nMaximum of 50 checks per request."
            }
          }
        ],
        "tags": [
          "BatchCheck"
        ]
      }
    },
    "/v1/namespace/{namespace}/check": {
      "post": {
        "summary": "Check access for subject on resource given its parent folder",
        "description": "This API checks if a subject is allowed to perform a given action on a specific resource.",
        "operationId": "Check",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CheckResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "namespace",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "subject": {
                  "type": "string"
                },
                "verb": {
                  "type": "string"
                },
                "group": {
