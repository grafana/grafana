{
  "type": "datasource",
  "name": "TestData",
  "id": "grafana-testdata-datasource",
  "aliasIDs": ["testdata"],
  "executable": "gpx_testdata",

  "metrics": true,
  "logs": true,
  "alerting": true,
  "annotations": true,
  "backend": true,

  "queryOptions": {
    "minInterval": true,
    "maxDataPoints": true
  },

  "info": {
    "description": "Generates test data in different forms",
    "author": {
      "name": "Grafana Labs",
      "url": "https://grafana.com"
    },
    "logos": {
      "small": "img/testdata.svg",
      "large": "img/testdata.svg"
    },
    "version": "%VERSION%",
    "links": [
      { "name": "Raise issue", "url": "https://github.com/grafana/grafana/issues/new" },
      { "name": "Documentation", "url": "https://grafana.com/docs/grafana/latest/datasources/testdata/" }
    ]
  },

  "includes": [
    {
      "type": "dashboard",
      "name": "Streaming Example",
      "path": "dashboards/streaming.json"
    }
  ],

  "dependencies": {
    "grafanaDependency": ">=11.6.11 <12 || >=12.0.10 <12.1 || >=12.1.7 <12.2 || >=12.2.5 <12.3 || >=12.3.0",
    "plugins": []
  }
}
