    "@grafana/runtime": "workspace:*",
    "@grafana/ui": "workspace:*",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-use": "17.5.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@grafana/plugin-configs": "workspace:*",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.14.202",
