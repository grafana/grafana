      "addToNav": false,
      "defaultNav": false
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.3.3",
    "plugins": []
  },
  "generated": {
    "extensions": [
      {
        "extensionPointId": "plugins/myorg-extensionpoint-app/actions",
        "title": "Go to A",
        "description": "Navigating to pluging A",
