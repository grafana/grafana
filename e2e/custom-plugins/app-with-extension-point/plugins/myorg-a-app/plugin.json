  "preload": true,
  "info": {
    "keywords": ["app"],
    "description": "Will extend root app with ui extensions",
    "author": {
      "name": "Myorg"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
