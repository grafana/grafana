go 1.22

toolchain go1.22.4

use (
	.
	./pkg/apimachinery
	./pkg/apiserver
	./pkg/build
	./pkg/build/wire
	./pkg/promlib
	./pkg/util/xorm
)

// when we release xorm we would like to release it like github.com/grafana/grafana/pkg/util/xorm
