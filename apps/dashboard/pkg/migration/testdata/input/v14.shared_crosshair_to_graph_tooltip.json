{
    "title": "V14 Shared Crosshair Migration Test Dashboard",
    "schemaVersion": 13,
    "sharedCrosshair": true,
    "panels": [
      {
        "type": "graph",
        "title": "CPU Usage Over Time",
        "id": 1,
        "targets": [
          {
            "expr": "cpu_usage",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "show": true,
            "min": null,
            "max": null
          }
        ]
      },
      {
        "type": "graph", 
        "title": "Memory Usage",
        "id": 2,
        "targets": [
          {
            "expr": "memory_usage",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "show": true,
            "min": 0,
            "max": 100
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "server",
          "type": "query",
          "datasource": "prometheus",
          "query": "label_values(server)",
          "refresh": 1,
          "options": []
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": []
    }
  }
