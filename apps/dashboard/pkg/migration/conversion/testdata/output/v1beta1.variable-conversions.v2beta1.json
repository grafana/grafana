      {
        "kind": "DatasourceVariable",
        "spec": {
          "name": "datasource_var",
          "pluginId": "prometheus",
          "refresh": "onDashboardLoad",
          "regex": "",
          "current": {
            "text": "Prometheus",
            "value": "prometheus-uid"
          },
          "options": [
            {
              "selected": true,
              "text": "Prometheus",
              "value": "prometheus-uid"
            },
            {
              "selected": false,
