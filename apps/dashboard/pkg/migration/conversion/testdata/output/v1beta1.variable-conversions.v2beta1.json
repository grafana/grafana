          "includeAll": true,
          "label": "Environment",
          "hide": "dontHide",
          "skipUrlSync": false,
          "description": "Deployment environment",
          "allowCustomValue": true
        }
      },
      {
        "kind": "AdhocVariable",
        "group": "prometheus",
        "datasource": {
          "name": "existing-ref-uid"
        },
        "spec": {
          "name": "adhoc_var",
          "baseFilters": [
            {
              "key": "environment",
              "operator": "=",
              "value": "production"
            }
          ],
          "filters": [
            {
              "key": "job",
              "operator": "=",
              "value": "prometheus"
            }
          ],
          "defaultKeys": [
            {
              "text": "job",
              "value": "job"
            },
            {
              "text": "instance",
              "value": "instance"
            }
          ],
          "label": "Filters",
          "hide": "dontHide",
          "skipUrlSync": false,
          "description": "Add filters",
          "allowCustomValue": true
        }
      },
