          "builtIn": 0,
          "enable": true,
          "hide": false,
          "iconColor": "purple",
          "name": "No Datasource Annotation",
          "target": {
            "expr": "static_annotation_query",
            "refId": "Anno"
          },
          "type": "manual"
        },
        {
          "builtIn": 0,
          "datasource": {
            "type": "prometheus",
            "uid": "existing-ref-uid"
          },
          "enable": false,
          "hide": true,
          "iconColor": "gray",
