                      "reduce": {
                        "include": [
                          "id"
                        ],
                        "reducer": "count"
                      },
                      "replaceFields": false
                    }
                  }
                },
                {
                  "kind": "sortBy",
                  "spec": {
                    "id": "sortBy",
                    "options": {
                      "fields": {},
                      "sort": [
                        {
                          "desc": true,
                          "field": "tsNs"
                        }
                      ]
                    }
                  }
                },
                {
                  "kind": "limit",
                  "spec": {
                    "id": "limit",
                    "options": {
                      "limitField": 10
                    }
                  }
                }
              ],
              "queryOptions": {}
            }
          },
          "vizConfig": {
            "kind": "VizConfig",
            "group": "logs",
            "version": "",
            "spec": {
              "options": {
                "dedupStrategy": "none",
                "enableLogDetails": true,
