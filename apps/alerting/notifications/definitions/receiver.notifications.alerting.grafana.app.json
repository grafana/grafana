{
  "kind": "CustomResourceDefinition",
  "apiVersion": "apiextensions.k8s.io/v1",
  "metadata": {
    "name": "receivers.notifications.alerting.grafana.app"
  },
  "spec": {
    "group": "notifications.alerting.grafana.app",
    "versions": [
      {
        "name": "v0alpha1",
        "served": true,
        "storage": true,
        "schema": {
          "openAPIV3Schema": {
            "properties": {
              "spec": {
                "properties": {
                  "integrations": {
                    "items": {
                      "properties": {
                        "disableResolveMessage": {
                          "type": "boolean"
                        },
                        "secureFields": {
                          "additionalProperties": {
                            "type": "boolean"
                          },
                          "type": "object"
                        },
                        "settings": {
                          "type": "object",
                          "x-kubernetes-preserve-unknown-fields": true
                        },
                        "type": {
                          "type": "string"
                        },
                        "uid": {
                          "type": "string"
                        },
                        "version": {
                          "type": "string"
                        }
                      },
                      "required": ["type", "version", "settings"],
                      "type": "object"
                    },
                    "type": "array"
                  },
                  "title": {
                    "type": "string"
                  }
                },
                "required": ["title", "integrations"],
                "type": "object"
              }
            },
            "required": ["spec"],
            "type": "object"
          }
        },
        "selectableFields": [
          {
            "jsonPath": ".spec.title"
          }
        ]
      }
    ],
    "names": {
      "kind": "Receiver",
      "plural": "receivers"
    },
    "scope": "Namespaced"
  }
}
