{
    "apiVersion": "provisioning.grafana.app/v0alpha1",
    "kind": "Repository",
    "metadata": {
        "name": "s3-example"
    },
    "spec": {
        "description": "load resources from an S3 bucket",
        "s3": {
            "bucket": "my-bucket",
            "region": "us-west-1"
        },
        "sync": {
            "enabled": false,
            "target": "mirror",
            "intervalSeconds": 60
        },
        "readOnly": false,
        "title": "S3 Example",
        "type": "s3"
    }
}