{
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "name": "@grafana/traces",
  "version": "0.1.0",
  "description": "Library to manage traces in Grafana.",
  "keywords": [
    "typescript"
  ],
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "http://github.com/grafana/grafana.git",
    "directory": "packages/grafana-traces"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "access": "public"
  },
  "files": [
    "dist",
    "./README.md",
    "./CHANGELOG.md",
    "LICENSE_APACHE2"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && rollup -c rollup.config.ts",
    "bundle": "rollup -c rollup.config.ts",
    "clean": "rimraf ./dist ./compiled ./package.tgz",
    "typecheck": "tsc --emitDeclarationOnly false --noEmit",
    "prepack": "cp package.json package.json.bak && node ../../scripts/prepare-packagejson.js",
    "postpack": "mv package.json.bak package.json"
  },
  "devDependencies": {
    "@grafana/eslint-config": "6.0.1",
    "@grafana/eslint-plugin": "link:./packages/grafana-eslint-rules",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@rollup/plugin-commonjs": "25.0.2",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.4",
    "@types/react": "18.2.15",
    "@types/testing-library__jest-dom": "5.14.8",
    "esbuild": "0.18.12",
    "eslint-plugin-jest": "27.6.0",
    "glob": "^10.2.7",
    "jest": "29.7.0",
    "rimraf": "5.0.1",
    "rollup": "2.79.1",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-node-externals": "^5.0.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@grafana/data": "10.3.0-pre",
    "@grafana/experimental": "1.7.4",
    "@grafana/runtime": "10.3.0-pre",
    "@grafana/schema": "10.3.0-pre",
    "@grafana/ui": "10.3.0-pre",
    "react": "18.2.0",
    "react-use": "17.4.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.0"
  }
}
