{
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "name": "@grafana/schema",
  "version": "12.3.0-pre",
  "description": "Grafana Schema Library",
  "keywords": [
    "typescript"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "http://github.com/grafana/grafana.git",
    "directory": "packages/grafana-schema"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@grafana-app/source": "./src/index.ts",
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./index": {
      "@grafana-app/source": "./src/index.ts",
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./common/*": {
      "@grafana-app/source": "./src/common/*.ts",
      "types": "./dist/types/common/*.d.ts",
      "import": "./dist/esm/common/*.mjs",
      "require": "./dist/cjs/common/*.cjs"
    },
    "./raw/composable/*": {
      "@grafana-app/source": "./src/raw/composable/*.ts",
      "types": "./dist/types/raw/composable/*.d.ts",
      "import": "./dist/esm/raw/composable/*.mjs",
      "require": "./dist/cjs/raw/composable/*.cjs"
    },
    "./raw/preferences": {
      "@grafana-app/source": "./src/raw/preferences/x/preferences_types.gen.ts",
      "types": "./dist/types/raw/preferences/x/preferences_types.gen.d.ts",
      "import": "./dist/esm/raw/preferences/x/preferences_types.gen.mjs",
      "require": "./dist/cjs/raw/preferences/x/preferences_types.gen.cjs"
    },
    "./raw/dashboard": {
      "@grafana-app/source": "./src/raw/dashboard/x/dashboard_types.gen.ts",
      "types": "./dist/types/raw/dashboard/x/dashboard_types.gen.d.ts",
      "import": "./dist/esm/raw/dashboard/x/dashboard_types.gen.mjs",
      "require": "./dist/cjs/raw/dashboard/x/dashboard_types.gen.cjs"
    },
    "./raw/librarypanel": {
      "@grafana-app/source": "./src/raw/librarypanel/x/librarypanel_types.gen.ts",
      "types": "./dist/types/raw/librarypanel/x/librarypanel_types.gen.d.ts",
      "import": "./dist/esm/raw/librarypanel/x/librarypanel_types.gen.mjs",
      "require": "./dist/cjs/raw/librarypanel/x/librarypanel_types.gen.cjs"
    },
    "./schema/dashboard/*": {
      "@grafana-app/source": "./src/schema/dashboard/*.ts",
      "types": "./dist/types/schema/dashboard/*.d.ts",
      "import": "./dist/esm/schema/dashboard/*.mjs",
      "require": "./dist/cjs/schema/dashboard/*.cjs"
    },
    "./veneer/*": {
      "@grafana-app/source": "./src/veneer/*.types.ts",
      "types": "./dist/types/veneer/*.types.d.ts",
      "import": "./dist/esm/veneer/*.types.mjs",
      "require": "./dist/cjs/veneer/*.types.cjs"
    },
    "./dist/*": {
      "types": "./dist/*",
      "default": "./dist/*"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "./README.md",
    "./CHANGELOG.md",
    "LICENSE_APACHE2"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && rollup -c rollup.config.ts --configPlugin esbuild",
    "bundle": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./dist ./compiled ./package.tgz",
    "typecheck": "tsc --emitDeclarationOnly false --noEmit",
    "prepack": "cp package.json package.json.bak && node ../../scripts/prepare-npm-package.js",
    "postpack": "mv package.json.bak package.json"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "16.0.1",
    "esbuild": "0.25.8",
    "glob": "^11.0.0",
    "rimraf": "6.0.1",
    "rollup": "^4.22.4",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "tslib": "2.8.1"
  }
}
