{
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "name": "@grafana/test-utils",
  "version": "12.0.0-pre",
  "private": true,
  "description": "Grafana test utils & Mock API",
  "keywords": [
    "grafana",
    "typescript",
    "msw",
    "mock api"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "http://github.com/grafana/grafana.git",
    "directory": "packages/grafana-test-utils"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "module": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./server": {
      "import": "./src/server/index.ts",
      "require": "./src/server/index.ts"
    },
    "./worker": {
      "import": "./src/worker/index.ts",
      "require": "./src/worker/index.ts"
    },
    "./handlers": {
      "import": "./src/handlers/index.ts",
      "require": "./src/handlers/index.ts"
    },
    "./unstable": {
      "import": "./src/unstable.ts",
      "require": "./src/unstable.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --emitDeclarationOnly false --noEmit"
  },
  "dependencies": {
    "msw": "2.7.0"
  },
  "devDependencies": {
    "@grafana/tsconfig": "^2.0.0",
    "@types/node": "22.12.0",
    "typescript": "5.7.3"
  }
}
