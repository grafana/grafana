    "@types/rollup-plugin-visualizer": "2.6.0",
    "@types/sinon": "^7.5.2",
    "pretty-format": "25.1.0",
    "rollup": "2.33.3",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "rollup-plugin-visualizer": "4.2.0",
    "sinon": "8.1.1",
    "tinycolor2": "1.4.1",
    "typescript": "4.2.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
